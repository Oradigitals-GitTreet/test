BEGIN ALR_ALERTS "XXADIB" "MPM Lease Expire before 120 Days"
  OWNER = "ORACLE12.2.13"
  ALERT_CONDITION_TYPE = "P"
  ENABLED_FLAG = "Y"
  START_DATE_ACTIVE = "2015/08/18 00:00:00"
  DESCRIPTION = "MPM Lease Expire before 120 Days"
  FREQUENCY_TYPE = "C"
  DAYS_BETWEEN_CHECKS = "1"
  DATE_LAST_CHECKED = "2025/11/06 16:17:39"
  MAINTAIN_HISTORY_DAYS = "4"
  CHECK_START_TIME = "66600"
  SQL_STATEMENT_TEXT =
 "SELECT LEASE_NUM\n\
      ,LOCATION_CODE\n\
      ,TOTAL_LEASE_RENT\n\
      ,TOTAL_UNITS\n\
      ,LEASE_DATE\n\
      ,LEASE_EXPIRY_DATE\n\
      ,CUSTOMER_NAME\n\
      ,EMAIL_ID\n\
      ,PH_NUM\n\
      ,TEXT\n\
  INTO &le_num\n\
      ,&loc_code\n\
      ,&unit_rent\n\
      ,&total_units\n\
      ,&start_date\n\
      ,&end_date\n\
      ,&te_name\n\
      ,&te_mail\n\
      ,&te_mnum\n\
      ,&p_text\n\
  FROM (SELECT DISTINCT\n\
               pla.lease_num\n\
              ,ploca.location_code\n\
              , (SELECT NVL (\n\
                           SUM (\n\
                              NVL (\n\
                                 XXMPM_GRADING_ERV_UTILS_PKG.XXMPM_GET_PRICELIST (\n\
                                    pta.location_id\n\
                                   ,'REN'\n\
                                   ,SYSDATE)\n\
                                ,0))\n\
                          ,0)\n\
                   FROM PN_TENANCIES_ALL PTA\n\
                  WHERE     PTA.LEASE_ID = PLA.LEASE_ID\n\
                        AND PLA.ORG_ID = 121\n\
                        AND PTA.EXPIRATION_DATE =\n\
                               TRUNC (plda.lease_termination_date))\n\
                  total_lease_rent\n\
              , (SELECT COUNT (pta.location_id)\n\
                   FROM PN_TENANCIES_ALL PTA\n\
                  WHERE     PTA.LEASE_ID = PLA.LEASE_ID\n\
                        AND PLA.ORG_ID = 121\n\
                        AND PTA.EXPIRATION_DATE =\n\
                               TRUNC (plda.lease_termination_date))\n\
                  total_units\n\
              ,NVL (\n\
                  (SELECT change_commencement_date\n\
                     FROM pn_lease_changes_all a\n\
                    WHERE     a.lease_id = pla.lease_id\n\
                          AND a.lease_change_number =\n\
                                 (SELECT MAX (x.lease_change_number)\n\
                                    FROM pn_lease_changes_all x\n\
                                   WHERE     x.lease_id = a.lease_id\n\
                                         AND x.change_type_lookup_code =\n\
                                                'AMEND')\n\
                          AND a.change_type_lookup_code = 'AMEND'\n\
                          AND ROWNUM = 1)\n\
                 ,plda.lease_commencement_date)\n\
                  lease_date\n\
              ,NVL (plda.lease_termination_date, SYSDATE)\n\
                  AS lease_expiry_date\n\
              ,cust.customer_name\n\
              ,email_id\n\
              ,p_country_code || p_area_code || p_number PH_NUM\n\
              ,   '\"'\n\
              || 'Dear '\n\
               || cust.customer_name\n\
               || ', Your Lease:'\n\
               || pla.lease_num\n\
               || ' expires on:'\n\
               || plda.lease_termination_date\n\
               || ' for the unit:'\n\
               || ploca.location_code\n\
               || '. Reminding you to renewal your lease ASAP'\n\
               || ' and the Rental Value {'\n\
               || (SELECT NVL (\n\
                             SUM (\n\
                                NVL (\n\
                                   XXMPM_GRADING_ERV_UTILS_PKG.XXMPM_GET_PRICELIST (\n\
                                      pta.location_id\n\
                                     ,'REN'\n\
                                     ,SYSDATE)\n\
                                  ,0))\n\
                            ,0)\n\
                     FROM PN_TENANCIES_ALL PTA\n\
                    WHERE     PTA.LEASE_ID = PLA.LEASE_ID\n\
                          AND PLA.ORG_ID = 121\n\
                          AND PTA.EXPIRATION_DATE =\n\
                                 TRUNC (plda.lease_termination_date))\n\
               || '}'\n\
               || '\"'\n\
                  text\n\
              , (SELECT COUNT (pta.location_id)\n\
                   FROM PN_TENANCIES_ALL PTA\n\
                  WHERE     PTA.LEASE_ID = pla.lease_id\n\
                        AND PTA.ORG_ID = 121\n\
                        AND NVL (XXMPM_GRADING_ERV_UTILS_PKG.XXMPM_GET_PRICELIST (pta.location_id,'REN',SYSDATE),0) = 0\n\
                        AND PTA.EXPIRATION_DATE = TRUNC (plda.lease_termination_date)\n\
                        AND pta.TENANCY_USAGE_LOOKUP_CODE !='PRKSPC')\n\
                  zero_count\n\
          FROM pn_leases_all pla\n\
              ,pn_lease_details_all plda\n\
              ,pn_locations_pub_v ploca\n\
              ,xxmpm_custmor_cont_temp cont\n\
              ,xxmpm_custmor_hdr_temp cust\n\
              ,hz_cust_accounts hca\n\
         WHERE     1 = 1\n\
               AND LENGTH (p_number) >= 4\n\
               AND (email_id IS NOT NULL OR p_number IS NOT NULL)\n\
               AND pla.lease_id = plda.lease_id\n\
               AND TRUNC (plda.lease_termination_date) - TRUNC (SYSDATE) =\n\
                      (SELECT TO_NUMBER (meaning)\n\
                         FROM FND_LOOKUP_VALUES_VL\n\
                        WHERE     lookup_type = 'MPM_RENEWAL_NOTICE_DAYS'\n\
                              AND lookup_code = 1\n\
                              AND TRUNC (SYSDATE) BETWEEN start_date_active\n\
                                                      AND NVL (\n\
                                                             end_date_active\n\
                                                            ,TRUNC (SYSDATE))\n\
                              AND enabled_flag = 'Y')\n\
               AND ploca.location_id = pla.location_id\n\
               and ploca.region not in ('Northern Emirates','Southern Emirates')\n\
               -- COnditions\n\
               AND NOT EXISTS\n\
                      (SELECT 'Under Legal'\n\
                         FROM xxmpm_legal_case_hdr_tl\n\
                        WHERE     1 = 1\n\
                              AND case_status NOT IN ('Draft',\n\
                              'Closed'\n\
                                                     ,'Rejected'\n\
                                                     ,'Canceled')\n\
                              AND NVL (case_reason, 'OT') NOT IN ('Execution Cheques','Cheque Bounce')\n\
                              AND lease_id =\n\
                                     (SELECT MAX (plas.lease_id)\n\
                                        FROM pn_leases_all plas\n\
                                            ,pn_tenancies_all ptas\n\
                                       WHERE     1 = 1\n\
                                             AND plas.lease_status = 'ACT'\n\
                                             AND plas.lease_id =\n\
                                                    ptas.lease_id\n\
                                             AND ptas.location_id =\n\
                                                    pla.location_id)\n\
                              AND ROWNUM = 1)\n\
               AND PLA.LEASE_TYPE_CODE NOT IN ('INV', 'INVO')\n\
               AND NOT EXISTS (\n\
                               SELECT 1\n\
                                 FROM PN_LOCATIONS_ALL PLAL\n\
                                WHERE PLAL.LOCATION_ID = PLOCA.LOCATION_ID\n\
                                  AND PLAL.LOCATION_TYPE_LOOKUP_CODE = 'OFFICE'\n\
                                  AND PLOCA.ATTRIBUTE14 = 'Suspended')\n\
               AND NOT EXISTS\n\
                      (SELECT 1\n\
                         FROM XXMPM_SPC_INSTRUCTION_HDR SI\n\
                        WHERE     NVL (SI.INS_TYPE, 'OTHER') = 'STR'\n\
                              AND SI.LOCATION_id = PLA.LOCATION_ID\n\
                              AND SI.DISABLE_DATE IS NULL)\n\
               AND pla.customer_id = hca.cust_account_id\n\
               AND cust.customer_number = hca.account_number\n\
               AND cont.customer_id(+) = cust.customer_id\n\
               AND cont.primary_flag = 1\n\
               AND PLA.LEASE_STATUS = 'ACT'\n\
               AND PLA.ORG_ID = 121\n\
               AND XXMPM_PN_COMMON_UTILS.get_lease_property_id (pla.lease_id) NOT IN (SELECT property_id\n\
                                                                                        FROM XXMPM_RLSE_TO_LANDLORDS_TL\n\
                                                                                       WHERE status IN ('Draft'\n\
                                                                                                       ,'Approved'\n\
                                                                                                       ,'Approval In Process'\n\
                                                                                                       ,'Closed'\n\
                                                                                                       ,'Refund Completed'\n\
                                                                                                       ,'Refund In Process'))\n\
               AND NOT EXISTS\n\
                      (SELECT 1\n\
                         FROM xxmpm_lease_decision_all xlda\n\
                        WHERE     xlda.lease_id = pla.lease_id\n\
                              AND xlda.decision_type IN ('REN', 'EXT')\n\
                              AND xlda.status NOT IN ('REJ', 'CAN')\n\
                              AND xlda.ren_start_date > TRUNC (SYSDATE))\n\
               AND NOT EXISTS\n\
                      (SELECT 1\n\
                         FROM xxmpm_lease_decision_all xlda\n\
                        WHERE     xlda.lease_id = pla.lease_id\n\
                              AND xlda.decision_type IN ('TER', 'ETER')\n\
                              AND xlda.status NOT IN ('REJ', 'CAN'))\n\
               AND (SELECT COUNT (pta.location_id)\n\
                      FROM PN_TENANCIES_ALL PTA\n\
                     WHERE     PTA.LEASE_ID = PLA.LEASE_ID\n\
                           AND PTA.ORG_ID = PLA.ORG_ID\n\
                           AND PTA.EXPIRATION_DATE =\n\
                                  TRUNC (plda.lease_termination_date)) > 0)\n\
 WHERE ZERO_COUNT = 0"
  LAST_UPDATE_DATE = "2025/11/24"


